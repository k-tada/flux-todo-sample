{
  "name": "react_template",
  "version": "0.0.1",
  "description": "React + Webpack + Bable Template",
  "main": "index.js",
  "scripts": {
    "clean:www": "rm -rf dist",
    "clean": "npm run clean:www",
    "copy:html": "cp src/index.html www/",
    "copy": "npm run copy:html",
    "build": "npm run clean && npm run copy && webpack",
    "watch": "npm run clean && npm run copy && webpack --watch & `npm bin`/browser-sync start --config bs-config.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "flux": "^2.1.1",
    "react": "~0.14.7",
    "react-addons-update": "^0.14.8",
    "react-bootstrap": "^0.29.4",
    "react-css-modules": "^3.7.6",
    "react-dom": "~0.14.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^0.19.1",
    "webpack": "~1.12.12"
  }
}
